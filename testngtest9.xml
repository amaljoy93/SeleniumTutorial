<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite">
	 <test name="Sample Test for cross browser testing firefox">
	    <parameter name="browser" value="firefox"/>
         <classes>
             <class name="selenium.testng.test12.TestNGParallelBrowserTesting"/>
          </classes>
     </test>
     <test name="Sample Test for cross browser testing chrome">
	    <parameter name="browser" value="chrome"/>
         <classes>
             <class name="selenium.testng.test12.TestNGParallelBrowserTesting"/>
          </classes>
     </test>
     <test name="Sample Test for cross browser testing edge">
	    <parameter name="browser" value="edge"/>
         <classes>
             <class name="selenium.testng.test12.TestNGParallelBrowserTesting"/>
          </classes>
     </test> 
</suite> 